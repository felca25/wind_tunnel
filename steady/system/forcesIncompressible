forcesIncompressible
{
    type          forces;

    libs          ("libforces.so");

    writeControl  timeStep;
    timeInterval  1;

    log           true;

    patches       (rotor);
    rho           rhoInf;     // Indicates incompressible
    // log           true;
    rhoInf        1.293;          // Redundant for incompressible

    CofR          (0 0 0);    // Rotation around centre line of propeller
    pitchAxis     (1 0 0);
}


