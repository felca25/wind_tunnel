FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PBiCGStab;
        // smoother        DICGaussSeidel;
        preconditioner  DIC;
        tolerance       1e-6;
        relTol          0.01;
        cacheAgglomeration no;
        minIter         3;
        maxIter         100;
    }

    pFinal
    {
        solver GAMG;
        tolerance 1e-06;
        relTol 0.0;
        smoother GaussSeidel;
        nPreSweeps 0;
        nPostSweeps 2;
        cacheAgglomeration on;
        agglomerator faceAreaPair;
        nCellsInCoarsestLevel 1000;
        mergeLevels 1;
    }

    // pFinal
    // {
    //     $p;
    //     nPreSweeps 0;
    //     nPostSweeps 2;
    //     cacheAgglomeration on;
    //     agglomerator faceAreaPair;
    //     nCellsInCoarsestLevel 1000;
    //     mergeLevels     1;
    //     relTol          0.0;
    // }

    "pcorr.*"
    {
        $p;
        tolerance       1e-2;
        relTol          0;
    }

    MeshPhi
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-2;
        relTol          0.0;
    }

    "(U|k|epsilon|omega)"
    {
        solver PBiCGStab;
        preconditioner DILU;
        minIter         3;
        tolerance       1e-8;
        relTol          0.0;
    }

    "(U|k|epsilon|omega)Final"
    {
        $U;
        relTol          0;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 3;
    consistent yes;
}

potentialFlow
{
    nNonOrthogonalCorrectors 10;
}

PIMPLE
{
    correctPhi          yes;
    correctMeshPhi      yes;
    nOuterCorrectors    2;
    nCorrectors         3;
    nNonOrthogonalCorrectors 3;
}

relaxationFactors
{
    fields
    {
        p               0.3;
    }
    equations
    {
        "(U|k|epsilon|omega)"   0.7;
        "(U|k|epsilon|omega)Final" 0.9;
    }
}

cache
{
    grad(U);
}

